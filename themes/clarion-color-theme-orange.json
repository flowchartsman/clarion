    "tokenColors": [
        {
            "scope": [
                "comment.block",
                "comment.block.documentation",
                "comment.line",
                "comment",
                "string.quoted.docstring"
            ],
            "settings": {
                "foreground": "#866c00",
                "fontStyle": "italic",
            }
        },
        {
            "scope":[
              "source.go constant.other.placeholder.go",
            ],
            "settings":{
              "fontStyle": "bold underline"
            }
        },
        {
            "scope": [
                "source.go constant.character.escape.go"
            ],
            "settings":{
                "foreground": "#866c00",
            }
        },  
        {
            "scope": [
                "punctuation.definition.string",
                "storage.type.string.python"
            ],
            "settings": {
                "foreground": "#000000"
            }
        },
        {
            "scope": [
                "markup.italic",
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "string",
                "entity.name.section.group-title",
                "keyword.const",
                //"keyword.function",
                "keyword.import",
                "keyword.var",
                "markup.heading",
                "storage.type.function",
                "storage.type.import",
                "storage.type.namespace",
                "storage.type.string.python",
                "support.type.object.module",
                "markup.bold",
                "markup.inline.raw",
                "markup.raw.monospace",
                "markup.fenced_code.block",
            ],
            "settings": {
                //"foreground":"#866c00",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "support.type.property-name",
            ],
            "settings" : {
                "fontStyle": ""
            }
        }
    ]
}
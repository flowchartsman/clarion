    "semanticTokenColors": {
        "function.definition": {
            "fontStyle": "bold"
        },
        "type.definition": {
            "fontStyle": "bold"
        },
        "variable.definition": {
            "foreground": "#00000094"
        },
        "variable.readonly": {
            "foreground": "#000000",
            "fontStyle": "italic"
        },
        "variable.defaultLibrary": {
            "foreground": "#000000",
            "fontStyle": "italic"
        },
        "variable": {}
    },
    "tokenColors": [
        {
            "scope": [
                "string entity.name.import.go"
            ],
            "settings": {
                "foreground": "#000000"
            }
        },
        {
            "scope": [
                "comment.block",
                "comment.block.documentation",
                "comment.line",
                "comment",
                "string.quoted.docstring"
            ],
            "settings": {
                "foreground": "#0000004d",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "string"
            ],
            "settings": {
                "foreground": "#dd0000"
            }
        },
        {
            "scope": [
                "constant.other.placeholder",
                "constant.character.escape"
            ],
            "settings": {
                "foreground": "#000000",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "punctuation.definition.string",
                "storage.type.string.python"
            ],
            "settings": {
                "foreground": "#000000"
            }
        },
        {
            "scope": [
                "markup.italic"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "entity.name.section.group-title",
                "keyword.const",
                "keyword.package",
                "markup.heading",
                "storage.type.function",
                "storage.type.import",
                "storage.type.namespace",
                "support.type.object.module",
                "markup.bold",
                "markup.inline.raw",
                "markup.raw.monospace",
                "markup.fenced_code.block",
                "keyword.control.landmark"
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "support.type.property-name"
            ],
            "settings": {
                "foreground": "#000000",
                "fontStyle": "bold"
            }
        }
    ]

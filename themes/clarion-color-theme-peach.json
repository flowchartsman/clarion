    "tokenColors": [
        {
            "scope": [
                "comment.block",
                "comment.block.documentation",
                "comment.line",
                "comment",
                "string.quoted.docstring"
            ],
            "settings": {
                "foreground": "#6e5a3e",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "string"
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "constant.other.placeholder"
            ],
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "scope": [
                "constant.character.escape"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "punctuation.definition.string",
                "storage.type.string.python"
            ],
            "settings": {
                "foreground": "#000000"
            }
        },
        {
            "scope": [
                "markup.italic"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "entity.name.section.group-title",
                "keyword.const",
                "keyword.function",
                "keyword.import",
                "keyword.var",
                "markup.heading",
                "storage.type.function",
                "storage.type.import",
                "storage.type.namespace",
                "support.type.object.module",
                "markup.bold",
                "markup.inline.raw",
                "markup.raw.monospace",
                "markup.fenced_code.block",
                "keyword.type"
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "support.type.property-name"
            ],
            "settings": {
                "foreground": "#000000",
                "fontStyle": "bold"
            }
        }
    ]

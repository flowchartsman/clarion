    "tokenColors": [
        {
            "scope": [
                "comment.block",
                "comment.block.documentation",
                "comment.line",
                "comment",
                "string.quoted.docstring"
            ],
            "settings": {
                "foreground": "#6e5a3e",
                "fontStyle": "italic",
            }
        },
        {
            "scope":[
              "source.go constant.other.placeholder.go",
            ],
            "settings":{
              "fontStyle": "bold underline"
            }
        },
        {
            "scope": [
                "source.go constant.character.escape.go"
            ],
            "settings":{
                "foreground": "#6e5a3e",
            }
        },  
        {
            "scope": [
                "punctuation.definition.string",
                "storage.type.string.python"
            ],
            "settings": {
                "foreground": "#000000"
            }
        },
        {
            "scope": [
                "string",
                "entity.name.section.group-title",
                "keyword.const",
                //"keyword.function",
                "keyword.import",
                "keyword.var",
                "markup.heading",
                "storage.type.function",
                "storage.type.import",
                "storage.type.namespace",
                "storage.type.string.python",
                "support.type.object.module",
                "markup.inline.raw",
                "markup.raw.monospace.asciidoc",
            ],
            "settings": {
                //"foreground":"#6e5a3e",
                "fontStyle": "bold"
            }
        }
    ]

{
    "name": "clarion",
    "displayName": "clarion",
    "description": "A minimalist theme focusing on clarity and semantic content.",
    "publisher": "flowchartsman",
    "repository": "https://github.com/flowchartsman/clarion",
    "license": "MIT",
    "icon": "icon.png",
    "keywords": [
        "monochrome",
        "light",
        "clarity",
        "readable",
        "readability",
        "contrast",
        "science"
    ],
    "version": "0.0.1-3-g3381bc7",
    "engines": {
        "vscode": "^1.55.0"
    },
    "categories": [
        "Themes"
    ],
    "contributes": {
        "grammars": [
            {
                "path": "./syntaxes/landmarks.tmLanguage.json",
                "scopeName": "clarion.landmarks",
                "injectTo": ["source.go","source.c","source.cpp"]
            }
        ],
        "themes": [
            {
                "label": "Clarion White",
                "uiTheme": "vs",
                "path": "./themes/clarion-color-theme-white.json"
            },
            {
                "label": "Clarion Orange",
                "uiTheme": "vs",
                "path": "./themes/clarion-color-theme-orange.json"
            },
            {
                "label": "Clarion Red",
                "uiTheme": "vs",
                "path": "./themes/clarion-color-theme-red.json"
            },
            {
                "label": "Clarion Blue",
                "uiTheme": "vs",
                "path": "./themes/clarion-color-theme-blue.json"
            },
            {
                "label": "Clarion Peach",
                "uiTheme": "vs",
                "path": "./themes/clarion-color-theme-peach.json"
            }
        ]
    }
}